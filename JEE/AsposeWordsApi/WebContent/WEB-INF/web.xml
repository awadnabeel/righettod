<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>AsposeWordsApi</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Simple word document generator</description>
    <display-name>DocumentGenerator</display-name>
    <servlet-name>DocumentGenerator</servlet-name>
    <servlet-class>com.drighetto.essai.asposewordsapi.DocumentGenerator</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DocumentGenerator</servlet-name>
    <url-pattern>/DocumentGenerator</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Simple document modifier</description>
    <display-name>DocumentModifier</display-name>
    <servlet-name>DocumentModifier</servlet-name>
    <servlet-class>com.drighetto.essai.asposewordsapi.DocumentModifier</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DocumentModifier</servlet-name>
    <url-pattern>/DocumentModifier</url-pattern>
  </servlet-mapping>
</web-app>